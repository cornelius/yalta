<h1>New presentation</h1>

<%= error_messages_for :presentation %>

<% form_for(@presentation) do |f| %>
  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Abstract</b><br />
    <%= f.text_area :abstract %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', presentations_path %>
